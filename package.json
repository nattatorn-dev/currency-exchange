    "lodash": "^4.13.1",
    "moment": "^2.18.1",
    "normalizr": "^2.0.1",
    "numbro": "^1.9.3",
    "pretty-error": "^2.0.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-ga": "^2.1.0",
    "react-hamburger-menu": "^0.0.4",
    "react-helmet": "^3.1.0",
    "react-number-input": "^15.4.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-dom": "^4.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-sticky": "^5.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.2",
