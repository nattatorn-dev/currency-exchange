    "file-loader": "^0.9.0",
    "history": "^2.0.0",
    "hoist-non-react-statics": "^1.2.0",
    "humps": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "invariant": "^2.2.1",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.5.3",
